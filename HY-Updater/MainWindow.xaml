        <ProgressBar x:Name="PrgUpdate" Height="20" Margin="10,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="294"/>
        <Button x:Name="BtnCancel" Content="취소" Margin="229,35,0,0" VerticalAlignment="Top" Width="75" HorizontalAlignment="Left" Click="BtnCancel_Click"/>
    </Grid>
</Window>
