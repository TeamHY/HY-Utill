        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HY_Updater"
        mc:Ignorable="d"
        Title="HY Updater" Height="94" Width="320" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closing="MainWindow_Closing" Loaded="MainWindow_Loaded">
