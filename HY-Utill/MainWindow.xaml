        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HY_Utill"
        mc:Ignorable="d"
        Title="HY Utill" Height="350" Width="525" ResizeMode="CanMinimize" Loaded="MainFormLoad">
    <Grid>
        <Button x:Name="btnStart" Content="설치" HorizontalAlignment="Left" Height="30" Margin="439,281,0,0" VerticalAlignment="Top" Width="70" Click="Button_Click"/>
        <ProgressBar x:Name="prgInstall" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="424" Margin="10,281,0,0"/>
        <Label x:Name="lblCurrentVersion" Content="기존 버전" Height="266" Margin="0,0,269,45" VerticalAlignment="Bottom" VerticalContentAlignment="Center" HorizontalAlignment="Right" Width="240" HorizontalContentAlignment="Center" FontSize="72" FontWeight="Bold"/>
        <Label x:Name="lblLatestVersion" Content="최신 버전" Height="266" Margin="0,0,10,45" VerticalAlignment="Bottom" VerticalContentAlignment="Center" HorizontalAlignment="Right" Width="240" HorizontalContentAlignment="Center" FontSize="72" FontWeight="Bold"/>
    </Grid>
</Window>
